          MODULE=texstudio-qt5
         VERSION=2.12.22
          SOURCE=${MODULE%-*}-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/texstudio-org/texstudio/archive/$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${MODULE%-*}-$VERSION
      SOURCE_VFY=sha256:101949620d055ab0224ecbb48b0ad38a7403f179832a28b86e4cb16f4c2adc46
