          MODULE=texstudio-qt5
         VERSION=2.12.8
          SOURCE=${MODULE%-*}-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/texstudio-org/texstudio/archive/2.12.8.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${MODULE%-*}-$VERSION
      SOURCE_VFY=sha256:3dec30ded3f36bb0591f8ed1b78b86fa23f276f5744925bc91075715c3f58062
