          MODULE=teTeX-texmf
         VERSION=2.0.2
        #VERSION=2.0.1
          SOURCE=tetex-texmf-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/tetex-texmf-$VERSION
   SOURCE_URL[0]=ftp://tug.ctan.org/tex-archive/systems/unix/teTeX/current/distrib/$SOURCE
   SOURCE_URL[1]=ftp://cam.ctan.org/tex-archive/systems/unix/teTeX/current/distrib/$SOURCE
