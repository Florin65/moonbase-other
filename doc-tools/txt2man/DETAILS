          MODULE=txt2man
         VERSION=1.4.8
	  SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://mvertes.free.fr/download/$SOURCE
