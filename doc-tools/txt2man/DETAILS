          MODULE=txt2man
         VERSION=1.7.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://github.com/mvertes/txt2man/archive/
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$MODULE-$VERSION
