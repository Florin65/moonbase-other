	  MODULE=ipw2200
	 VERSION=1.0.2
	  SOURCE=$MODULE-$VERSION.tgz
      SOURCE_URL=$SFORGE_URL/$MODULE/
      SOURCE_VFY=sha1:d2c7e0a4d2dfc6051bebdec3b4c9c3982c0bb537
	 ENTERED=20041115
