          MODULE=samba
         VERSION=4.4.2
          SOURCE=$MODULE-$VERSION.tar.gz
         SOURCE2=$MODULE-$VERSION-libsystemd.patch
   SOURCE_URL[0]=http://us1.samba.org/$MODULE/ftp/stable
   SOURCE_URL[1]=http://us2.samba.org/$MODULE/ftp/stable
   SOURCE_URL[2]=http://us3.samba.org/$MODULE/ftp/stable
