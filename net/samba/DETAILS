   SOURCE_URL[5]=ftp://de.samba.org/samba.org
   SOURCE_URL[6]=http://us1.samba.org/$MODULE/ftp/old-versions
     SOURCE2_URL=$PATCH_URL
      SOURCE_VFY=sha256:eaecd41a85ebb9507b8db9856ada2a949376e9d53cf75664b5493658f6e5926a
