	  MODULE=ipw2100
	 VERSION=0.62
	  SOURCE=$MODULE-$VERSION.tgz
       HOSTAPVER=0.2.5
	 SOURCE2=hostap-driver-$HOSTAPVER.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/
     SOURCE2_URL=http://hostap.epitest.fi/releases/
     SOURCE3_URL=$PATCH_URL/
