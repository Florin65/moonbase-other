          MODULE=tftp-hpa
         VERSION=0.39
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL=http://www.kernel.org/pub/software/network/tftp/$MODULE-$VERSION.tar.bz2
     SOURCE_VFY=sha1:42dbc5110ed23a648f905fb4c662b49903c4036c
