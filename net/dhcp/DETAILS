          MODULE=dhcp
         VERSION=4.4.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL[0]=ftp://ftp.isc.org/isc/$MODULE/$VERSION
      SOURCE_URL[1]=https//ftp.isc.org/isc/$MODULE/$VERSION
