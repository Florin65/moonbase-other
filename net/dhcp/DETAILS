          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=ftp://ftp.isc.org/isc/$MODULE/$VERSION
      SOURCE_VFY=sha256:6246c9b358759f6cdcc45104caaf76e732a211dbbbbf64a21f499c8db1298165
