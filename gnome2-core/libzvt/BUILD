(

   ./configure  --build=$BUILD					\
                --sysconfdir=/etc                               \
                --prefix=/opt/lunar/gnome/2.4			\
		--localstatedir=/var				\
		--infodir=/opt/lunar/gnome/2.4/share/info	\
		--mandir=/opt/lunar/gnome/2.4/share/man		&&

   default_make

) > $C_FIFO 2>&1
