          MODULE=Data-UUID
         VERSION=1.226
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=https://search.cpan.org/CPAN/authors/id/R/RJ/RJBS
      SOURCE_VFY=sha256:093d57ffa0d411a94bafafae495697db26f5c9d0277198fe3f7cf2be22996453
