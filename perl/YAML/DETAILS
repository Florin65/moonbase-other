          MODULE=YAML
         VERSION=1.24
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://search.cpan.org/CPAN/authors/id/T/TI/TINITA/
      SOURCE_VFY=sha256:b58d045a72a5e1ce714f1d42b64ccddbb055a4afac10b74972dba3b6a47dffb6
