        SOURCE_URL=$SFORGE_URL/$MODULE
       SOURCE_VFY=sha256:04abaa207f4bf4f453bea5bbdbcf6cf4bcdba3ed1c5160bfd732c6b8c70c6269
          WEB_SITE=http://tcl.sourceforge.net
