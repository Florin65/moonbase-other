        SOURCE_URL=$SFORGE_URL/$MODULE
       SOURCE_VFY=sha256:5196dbf6638e3df8d5c87b5815c8c2b758496eb6f0e41446596c9a4e638d87ed
          WEB_SITE=http://tcl.sourceforge.net
