          MODULE=ctypes
	 VERSION=0.6.3
	  SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$SFORGE_URL/ctypes/$SOURCE
	WEB_SITE=http://starship.python.net/crew/theller/ctypes/
