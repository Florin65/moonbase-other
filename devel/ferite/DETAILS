           MODULE=ferite
         VERSION=0.99.5
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$SFORGE_URL/ferite/$SOURCE
      MAINTAINER=nhudson@speakeasy.net
        WEB_SITE=http://www.ferite.org
           SHORT="Ferite is a high level scripting language"
