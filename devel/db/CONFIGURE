if ! grep -q "CONFIG_JAVA="  $MODULE_CONFIG; then
  if query "Enable java support?" n  ; then
    if ! module_installed j2sdk; then
      lin j2sdk
    fi
    echo 'OPTS="$OPTS --enable-java"' > $MODULE_CONFIG
    CONFIG_JAVA=y
  else
    CONFIG_JAVA=n
  fi
  echo "CONFIG_JAVA=$CONFIG_JAVA" >> $MODULE_CONFIG
fi

