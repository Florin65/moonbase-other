          MODULE=byacc
         VERSION=1.9-1
          SOURCE=$MODULE-$VERSION-src.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=ftp://mirrors.rcn.net/mirrors/sources.redhat.com/cygwin/release/$MODULE/$SOURCE
   SOURCE_URL[2]=http://programming.ccp14.ac.uk/ftp-mirror/programming/cygwin/pub/cygwin/release/$MODULE/$SOURCE
        WEB_SITE=unknown
         ENTERED=20020501
         UPDATED=20020501
           SHORT="BSD yacc is a parser generator."
      MAINTAINER="jacksonje@health.missouri.edu"

cat << EOF
BSD yacc converts a grammar description for a LALR context-free grammar into a 
C program to parse that grammar. It can be used to create a range of language 
parsers.
EOF
