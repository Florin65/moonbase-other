          MODULE=pinentry
         VERSION=0.9.7
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=ftp://ftp.gnupg.org/gcrypt/$MODULE/
      SOURCE_VFY=sha256:6398208394972bbf897c3325780195584682a0d0c164ca5a0da35b93b1e4e7b2
