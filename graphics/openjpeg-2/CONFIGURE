mquery BUILD_MJ2 "Build mj2 binaries?" y "-DBUILD_MJ2=ON" "-DBUILD_MJ2=OFF"
mquery BUILD_JPWL "Build JPWL library?" y "-DBUILD_JPWL=ON" "-DBUILD_JPWL=OFF"
mquery ENABLE_JPIP "Build jpip library? (Needed if yes to JPIP server" y "-DBUILD_JPIP=ON" "-DBUILD_JPIP=OFF"
mquery ENABLE_JPIP_SERVER "Build jpip server?" n "-DBUILD_JPIP_SERVER=ON" "-DBUILD_JPIP_SERVER=OFF"
