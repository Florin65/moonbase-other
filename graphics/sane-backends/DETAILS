          MODULE=sane-backends
         VERSION=1.0.25
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-1.0.24
      SOURCE_URL=https://alioth.debian.org/frs/download.php/file/3958
