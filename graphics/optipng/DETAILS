          MODULE=optipng
         VERSION=0.7.5
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/project/optipng/OptiPNG/$MODULE-$VERSION
      SOURCE_VFY=sha1:30b6c333d74fc0f5dc83004aace252fa3321368b
        WEB_SITE=http://optipng.sourceforge.net/
         ENTERED=20150216
         UPDATED=20150216
           SHORT="PNG optimizer"

cat << EOF
OptiPNG is a PNG optimizer that recompresses image files to a smaller size, without losing any information.
This program also converts external formats (BMP, GIF, PNM and TIFF) to optimized PNG, and performs PNG 
integrity checks and corrections. 
EOF
