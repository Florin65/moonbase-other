          MODULE=gegl
         VERSION=0.4.8
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=https://download.gimp.org/pub/gegl/${VERSION%.*}/
      SOURCE_VFY=sha256:719468eec56ac5b191626a0cb6238f3abe9117e80594890c246acdc89183ae49
