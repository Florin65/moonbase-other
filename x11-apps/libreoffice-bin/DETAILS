          MODULE=libreoffice-bin
         VERSION=6.2.1
          SOURCE=LibreOffice_${VERSION}_Linux_x86_rpm.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION.2
      SOURCE_URL=http://download.documentfoundation.org/libreoffice/stable/$VERSION/rpm/x86
      SOURCE_VFY=sha256:78ab607c2c288e624ee6b0b1bfb14bc55c695ab9e2d8f6c5e8bc4e7d42200218
        WEB_SITE=http://www.libreoffice.org
         ENTERED=20110330
         UPDATED=20190306
           SHORT="The open office suite for unix"

LDD_CHECK=off
ARCHIVE=off

cat << EOF
LibreOffice is the free power-packed Open Source personal productivity suite for
Windows, Macintosh and Linux, that gives you six feature-rich applications for
all your document production and data processing needs: Writer, Calc, Impress,
Draw, Math and Base.
EOF
