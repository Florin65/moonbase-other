          MODULE=dmenufm
         VERSION=2.0
          SOURCE=$MODULE-$VERSION.tar.gz
 SOURCE_URL_FULL=https://github.com/huijunchen9260/dmenufm/archive/v${VERSION}.tar.gz
      SOURCE_VFY=sha256:efb97270b4d07a5f23dc185aea28a110b33af6f76eefbd89b8b46e4561f67a91
