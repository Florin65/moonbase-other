          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/nautilus-$VERSION
   SOURCE_URL[0]=$GNOME_URL/sources/$MODULE/2.0/$SOURCE
