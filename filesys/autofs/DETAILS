          MODULE=autofs
         VERSION=4.1.0
          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=ftp://ftp.kernel.org/pub/linux/daemons/$MODULE/v4/$MODULE/$SOURCE
