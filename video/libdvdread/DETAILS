         MODULE=libdvdread
        VERSION=6.1.1
         SOURCE=$MODULE-$VERSION.tar.bz2
     SOURCE_URL=https://download.videolan.org/pub/videolan/libdvdread/$VERSION/
     SOURCE_VFY=sha256:3e357309a17c5be3731385b9eabda6b7e3fa010f46022a06f104553bf8e21796
