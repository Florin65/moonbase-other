          MODULE=xine-lib
         VERSION=1-rc6a
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL=$SFORGE_URL/xine/$SOURCE
     SOURCE_VFY=sha1:9359543ae3cc34f25af08ef42cbd13f08cae8398
        WEB_SITE=http://xinehq.de/
