          MODULE=sysvinit
         VERSION=2.91
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=http://download.savannah.gnu.org/releases/$MODULE
      SOURCE_VFY=sha256:dfaeb41bc72eae679dfdba2ffeaefe2c4fda0295cd8c8448a066e0049923e1f3
