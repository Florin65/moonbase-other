          MODULE=unrar
        MYMODULE=unrarsrc
         VERSION=6.0.4
          SOURCE=$MYMODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=https://www.rarlab.com/rar
      SOURCE_VFY=sha256:130197e495d6e2c2ee790a5beee123edeed642508be13f0159672e5397aca6c1
