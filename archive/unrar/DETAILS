          MODULE=unrar
        MYMODULE=unrarsrc
         VERSION=5.5.5
          SOURCE=$MYMODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
      SOURCE_URL=http://www.rarlab.com/rar
      SOURCE_VFY=sha256:a4553839cb2f025d0d9c5633816a83a723e3938209f17620c8c15da06ed061ef
