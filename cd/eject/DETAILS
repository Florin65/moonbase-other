          MODULE=eject
         VERSION=2.0.13
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://members.rogers.com/jefftranter/$SOURCE
   SOURCE_URL[1]=ftp://ftp.ibiblio.org/pub/Linux/utils/disk-management/$SOURCE
        WEB_SITE=http://www.pobox.com/~tranter/eject.html
         ENTERED=20010922
