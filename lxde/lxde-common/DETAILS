          MODULE=lxde-common
         VERSION=0.99.0
          SOURCE=$MODULE-$VERSION.tar.xz
         SOURCE2=$MODULE-$VERSION-dbus_update_environment.patch.xz
      SOURCE_URL=$SFORGE_URL/lxde
     SOURCE2_URL=$PATCH_URL
      SOURCE_VFY=sha256:43f17bf92e4695fdf2476c1964990d1d61c832726dd0778ae4cb462b9d29335c
