          MODULE=mailutils
         VERSION=3.12
          SOURCE=${MODULE}-${VERSION}.tar.xz
      SOURCE_URL=ftp://ftp.gnu.org/pub/gnu/mailutils/
      SOURCE_VFY=sha256:6d43fa217c4ac63f057de87890c562d170bb92bc402368b5fbc579e4c2b3a158
