          MODULE=mailutils
         VERSION=3.11
          SOURCE=${MODULE}-${VERSION}.tar.xz
      SOURCE_URL=ftp://ftp.gnu.org/pub/gnu/mailutils/
      SOURCE_VFY=sha256:2dec0920774907d33db5ad71d5766b8551ba2d903603e4a11e3521c67c3b18ef
