          MODULE=mailutils
         VERSION=3.10
          SOURCE=${MODULE}-${VERSION}.tar.xz
      SOURCE_URL=ftp://ftp.gnu.org/pub/gnu/mailutils/
      SOURCE_VFY=sha256:097e3dd529576e92a5400b623d0b5a31126c88dc60cc38d55f7fadd764cf559f
