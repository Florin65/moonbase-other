          MODULE=catdoc
         VERSION=0.93.3
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_URL=ftp://ftp.45.free.net/pub/$MODULE/$SOURCE
      SOURCE_VRY=sha1:cbba208080d9c2b6ec5577b7aef6a80d5da63290
        WEB_SITE=http://www.45.free.net/~vitus/ice/catdoc/
         ENTERED=20020504
         UPDATED=20041003
           SHORT="catdoc outputs MS Word, Excel and RTF Files as text."

cat << EOF
catdoc is program which reads one or more Microsoft word files and 
outputs text, contained inside them to standard output. Therefore it
does same work for .doc files, as unix cat command for plain ASCII 
files.
EOF
