          MODULE=tree
         VERSION=1.5.0
          SOURCE=$MODULE-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=ftp://mama.indstate.edu/linux/tree/$SOURCE
     SOURCE_VFY=md5:e0d090c564e7ea5afa16bac80620c7e0
        WEB_SITE=http://mama.indstate.edu/users/ice/tree/
         ENTERED=20020906
         UPDATED=20040818
           SHORT="recursive directory listing"

cat << EOF
Tree is a recursive directory listing program that produces a depth 
indented listing of files, which is colorized ala dircolors if the 
LS_COLORS environment variable is set and output is to tty. 
EOF
