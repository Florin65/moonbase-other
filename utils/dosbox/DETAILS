          MODULE=dosbox
         VERSION=0.61
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
      SOURCE_URL=$SFORGE_URL/$MODULE/$SOURCE
      SOURCE_VFY=md5:dcb549eb2a9e6fe6995b973ef9ac6e9f 
        WEB_SITE=http://dosbox.sourceforge.net/
         ENTERED=20040331
