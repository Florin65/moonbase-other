          MODULE=xfdesktop
          PKG=$MODULE
         VERSION=4.1.91
         RELEASE=4.1.91
          SOURCE=${PKG}-${VERSION}.tar.gz
   SOURCE_URL[0]="http://de.lunar-linux.org/xfce4/${SOURCE}"
   SOURCE_URL[1]="ftp://ftp.berlios.de/pub/xfce-goodies/${RELEASE}/${SOURCE}"
     SOURCE_VFY=md5:584be9387a0d15fdff72454395f8da1b
      MAINTAINER=jasper@lunar-linux.org
         WEBSITE=http://www.xfce.org
         ENTERED=20030715
         UPDATED=20041031
           SHORT="Desktop manager for XFce 4"

cat << EOF
xfdesktop -- Desktop manager for the XFce 4 desktop environment. Handles the
background image settings and provides a mouse menu when you click on the
desktop background.

EOF
