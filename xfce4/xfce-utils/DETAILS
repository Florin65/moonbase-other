          MODULE=xfce-utils
          PKG=$MODULE
         VERSION=4.0.6
         RELEASE=4.0.6
          SOURCE=${PKG}-${VERSION}.tar.gz
   SOURCE_URL[0]="http://moe.spline.de/xfce/${SOURCE}"
   SOURCE_URL[1]="ftp://ftp.berlios.de/pub/xfce-goodies/${RELEASE}/${SOURCE}"
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${PKG}-${VERSION}
     SOURCE_VFY=md5:d42a01aa03a9eafb9bc2cc546d988b4b
      MAINTAINER=jasper@lunar-linux.org
         WEBSITE=http://www.xfce.org
         ENTERED=20030715
         UPDATED=20040711
           SHORT="Essential utilities for XFce 4"

cat << EOF
xfce-utils -- Several essential utilities and scripts for XFce 4. Provides a
askbar, the about dialog, the run dialog, the user guide and several useful
scripts that are used by other XFce 4 components.

