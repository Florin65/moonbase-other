          MODULE=xfce4-session
          PKG=$MODULE
         VERSION=0.2.1
          SOURCE=${PKG}-${VERSION}.tar.gz
SOURCE_DIRECTORY=${BUILD_DIRECTORY}/${PKG}-${VERSION}
   SOURCE_URL[0]=ftp://ftp.unix-ag.org/user/bmeurer/xfce4/$MODULE/$SOURCE
     SOURCE_VFY=md5:164dfd2ab1d2615cde18c0e897636ae3
      MAINTAINER=jasper@lunar-linux.org
