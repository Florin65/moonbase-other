          MODULE=hunspell
         VERSION=1.4.1
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
 SOURCE_URL_FULL=http://github.com/hunspell/hunspell/archive/v$VERSION.tar.gz
      SOURCE_VFY=sha256:c4476aff0ced52eec334eae1e8d3fdaaebdd90f5ecd0b57cf2a92a6fd220d1bb
