          MODULE=rdesktop
	 VERSION=1.3.0
         #VERSION=1.2.0
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE
   SOURCE_URL[0]=$SFORGE_URL/rdesktop/$SOURCE
     SOURCE_VFY=md5:d8108781d227221a823c34b861486a5d
        WEB_SITE=http://www.rdesktop.org
         ENTERED=20010922
