          MODULE=beepmp
         VERSION=0.9.6
          SOURCE=bmp-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/beepmp/$SOURCE
SOURCE_DIRECTORY=$BUILD_DIRECTORY/bmp-$VERSION
      SOURCE_VFY=md5:f7826222e186a06d22ceb0cbc7d67521
