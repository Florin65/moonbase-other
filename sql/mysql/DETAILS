SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://ftp.sunet.se/pub/unix/databases/relational/mysql/Downloads/MySQL-3.23/$SOURCE
