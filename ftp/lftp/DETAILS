          SOURCE=$MODULE-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://the.wiretapped.net/mirrors/lftp/$SOURCE
   SOURCE_URL[1]=http://ftp.yars.free.net/pub/software/unix/net/ftp/client/$MODULE/$SOURCE
      SOURCE_VFY=md5:bc5ed223daa568fda9a01f89b5000612
        WEB_SITE=http://lftp.yar.ru/
         ENTERED=20010922
         UPDATED=20040124
        SHORT="LFTP is sophisticated ftp/http client, file transfer program."

cat << EOF
LFTP is sophisticated ftp/http client, file transfer program. Like BASH,
it has job control and uses readline library for input. It has
bookmarks, built-in mirror, can transfer several files in parallel. It
was designed with reliability in mind.
EOF
