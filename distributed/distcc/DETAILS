SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://distcc.samba.org/ftp/distcc/$SOURCE
        WEB_SITE=http://distcc.samba.org/
      SOURCE_VFY=md5:98cfd19a0329c9c3fdaee14098b2a9a5
