	 SOURCE2=$MODULE-$VERSION-experimental.patch
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=http://bitconjurer.org/BitTorrent/$SOURCE
  SOURCE2_URL[0]=$PATCH_URL/$SOURCE2
