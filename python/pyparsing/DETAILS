          MODULE=pyparsing
         VERSION=2.1.0
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/$MODULE/$MODULE-$VERSION/
      SOURCE_VFY=sha256:f6cb2bc85a491347c3c699db47f7ecc02903959156b4f92669ebf82395982901
