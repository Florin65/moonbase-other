          MODULE=pyparsing
         VERSION=2.0.3
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/$MODULE/$MODULE-$VERSION/
      SOURCE_VFY=sha256:06e729e1cbf5274703b1f47b6135ed8335999d547f9d8cf048b210fb8ebf844f
