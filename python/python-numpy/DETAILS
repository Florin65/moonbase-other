 SOURCE_URL_FULL=http://github.com/numpy/numpy/archive/v$VERSION.tar.gz
      SOURCE_VFY=sha256:65123e53a00b84d03a5a18ae049f4647d1fd100c9010ca1fdf997900ee13875d
