          MODULE=gtkhtml32
             MOD=gtkhtml
         VERSION=3.2.4
          SOURCE=$MOD-$VERSION.tar.bz2
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MOD-$VERSION
      SOURCE_URL=$GNOME_URL/sources/$MOD/3.2/
      SOURCE_VFY=sha1:91b597a38909bddf12de410d45d8f3dc46e95959
        WEB_SITE=http://www.gnome.org
