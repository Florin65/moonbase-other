         VERSION=2.2.2
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=http://download.osgeo.org/gdal/$VERSION/
      SOURCE_VFY=sha256:eb25d6ee85f4f5ac1d5581958f8c6eed9b1d50746f82866fe92e507541def35b
