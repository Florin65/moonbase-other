          MODULE=gdal
         VERSION=2.2.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=http://download.osgeo.org/gdal/$VERSION/
      SOURCE_VFY=sha256:927098d54083ac919a497f787b835b099e9a194f2e5444dbff901f7426b86066
