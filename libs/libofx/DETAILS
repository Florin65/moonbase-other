          MODULE=libofx
         VERSION=0.6.5
          SOURCE=$MODULE-$VERSION.tar.gz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-$VERSION
   SOURCE_URL[0]=$SFORGE_URL/$MODULE/$SOURCE
        WEB_SITE=http://libofx.sf.net
         ENTERED=20030820
         UPDATED=20031105
       MAINTAINER=nhudson@lunar-linux.org
           SHORT="libofx parser and an API designed to allow 
applications to very easily support OFX files"
cat << EOF
This is the LibOFX library. It is a parser and an API designed to allow 
applications to very easily support OFX command responses, usually 
provided by financial institutions for statement downloads
EOF
